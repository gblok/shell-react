{
  "restartable": "rs",
  "verbose": true,
  "watchInterval": 10000,
  "ignore": [
    "static"
  ],
  "events": {
    "restart": "Server restart..."
  },
  "watch": [
    "server"
  ],
  "env": {
    "NODE_ENV": "development",
    "DEBUG": true
  },
  "ext": "js json pug",
  "execMap": {
    "js": "babel-node"
  }
}